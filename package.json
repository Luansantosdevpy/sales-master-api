{
  "name": "sales-master-api",
  "version": "1.0.0",
  "description": "Api para controle de estoque e vendas",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "dev": "yarn ts-node-dev --clear --inspect --transpile-only --ignore node_modules -r dotenv/config src/server.ts service:",
    "migration:create": "sequelize migration:generate --name",
    "migration:run": "sequelize db:migrate",
    "ci:lint:check": "prettier --check src tests && yarn eslint src tests",
    "start": "node ./dist/server.js"
  },
  "author": "Luan Santos",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.0",
    "@types/sequelize": "^4.28.15",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.1",
    "typescript": "^5.2.2"
  }
}
